{
  "name": "victorpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "next build && touch out/.nojekyll && git add out/ && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "preview": "npm run build && rm -rf .preview && mkdir .preview && mv out .preview/victorpage && npx serve .preview"
  },
